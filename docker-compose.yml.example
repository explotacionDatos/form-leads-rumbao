version: '3.8'

services:
  app:
    container_name: form-leads-rumbao
    build: .
    restart: unless-stopped
    env_file: stack.env
    ports:
      - $NODE_LOCAL_PORT:$NODE_DOCKER_PORT
    stdin_open: true
    tty: true
    networks:
      - nginxproxymanager_default

networks:
  nginxproxymanager_default:
     external: true
